{
	// Vscode Font
	"editor.minimap.enabled": false,
	"editor.fontFamily": "'Victor Mono', Consolas, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 14,
	"editor.fontWeight": "bold",
	"editor.formatOnSave": true,
	// Vscode Workbench
	"workbench.activityBar.visible": true,
	// Vscode Features
	"editor.stickyTabStops": true,
	"editor.stickyScroll.enabled": true,
	// TypeScript
	"typescript.validate.enable": true,
	// Material Icon theme
	"material-icon-theme.folders.associations": {
		".env.development": "tune",
		".env.development.local": "tune",
		".env.developmentpiramide.local": "tune",
		".env.developmentoceanica.local": "tune",
		".env.quality": "tune",
		".env.quality.local": "tune",
		".env.qualitypiramide.local": "tune",
		".env.qualityoceanica.local": "tune",
		".env.production": "tune",
		".env.production.local": "tune",
		".env.productionpiramide.local": "tune",
		".env.productionoceanica.local": "tune",
		"domain": "stack",
		"application": "app",
		"infrastructure": "cobol",
		"adapters": "buildkite",
		"usecases": "shader",
		"presenters": "animation",
		"Car": "cobol",
		"Core": "core",
		"Home": "environment",
		"People": "class",
		"Pet": "stack",
		"Travel": "other",
		"Budget": "shader"
	},
	"material-icon-theme.files.associations": {},
	// Bracket pair coloraized configuration
	"brackets-pair-colorized-2.colors": [
		"#fafafa",
		"#9f51b6",
		"#f7c244",
		"#f07850",
		"#9cdd29",
		"#0098fa"
	],
	// Prettier
	"prettier.enable": true,
	"prettier.printWidth": 100,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.semi": true,
	"prettier.singleQuote": true,
	"prettier.bracketSpacing": true,
	"prettier.arrowParens": "always",
	"prettier.trailingComma": "es5",
	// Peacok Configuration
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#f59d53",
		"activityBar.background": "#f59d53",
		"activityBar.foreground": "#15202b",
		"activityBar.inactiveForeground": "#15202b99",
		"activityBarBadge.background": "#07783b",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#15202b99",
		"sash.hoverBorder": "#f59d53",
		"statusBar.background": "#f28123",
		"statusBar.foreground": "#15202b",
		"statusBarItem.hoverBackground": "#d5680d",
		"statusBarItem.remoteBackground": "#f28123",
		"statusBarItem.remoteForeground": "#15202b",
		"titleBar.activeBackground": "#f28123",
		"titleBar.activeForeground": "#15202b",
		"titleBar.inactiveBackground": "#f2812399",
		"titleBar.inactiveForeground": "#15202b99"
	},
	"peacock.color": "f28123"
}